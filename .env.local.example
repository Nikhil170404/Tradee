# ===========================================
# ProTrader AI - Environment Variables
# ===========================================
# Copy this file to .env.local and fill in your values
# NEVER commit .env.local to version control!

# Supabase (Get from https://supabase.com/dashboard)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Upstash Redis (Get from https://upstash.com)
UPSTASH_REDIS_REST_URL=your_upstash_redis_url_here
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token_here

# News API (Get from https://newsapi.org)
NEWS_API_KEY=your_newsapi_key_here

# Alpha Vantage (Get from https://www.alphavantage.co)
ALPHA_VANTAGE_API_KEY=your_alphavantage_key_here

# Python Service URL (Backend FastAPI server)
PYTHON_SERVICE_URL=http://localhost:8000

# Allowed CORS Origins for Python Service
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
